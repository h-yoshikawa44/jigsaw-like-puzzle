{
  "name": "jigsaw-like-puzzle",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.10",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "firebase": "^8.8.1",
    "konva": "^8.1.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-konva": "^17.0.2-5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0",
    "typescript": "^4.3.5",
    "use-image": "^1.0.5",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-check": "yarn lint:eslint && yarn check:prettier",
    "lint:eslint": "yarn run -s eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "check:prettier": "yarn run -s prettier --check \"./{public,src}/**/*.{js,jsx,ts,tsx,html,gql,graphql,json}\"",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn check:prettier --write",
    "preinstall": "typesync || echo 'skip typesync'",
    "prepare": "simple-git-hooks || echo 'Can not set git hooks'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.172",
    "@types/prettier": "^2.3.2",
    "@types/prop-types": "^15.7.4",
    "@types/react-router-dom": "^5.1.8",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/testing-library__user-event": "^4.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^8.3.0",
    "lint-staged": "^12.3.4",
    "postcss-normalize": "^10.0.1",
    "prettier": "^2.0.5",
    "simple-git-hooks": "^2.7.0",
    "typesync": "^0.8.0"
  },
  "volta": {
    "node": "14.17.3",
    "yarn": "1.22.17"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn run -s lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write --loglevel=error",
      "eslint --fix --quiet"
    ],
    "./**/*.{html,gql,graphql,json}": [
      "prettier --write --loglevel=error"
    ]
  }
}
