{
  "name": "jigsaw-like-puzzle",
  "version": "3.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "tsc",
    "lint-check": "npm run lint:eslint && npm run check:prettier",
    "lint:eslint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "check:prettier": "prettier --check \"./{public,src}/**/*.{js,jsx,ts,tsx,html,gql,graphql,json}\"",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run check:prettier -- --write",
    "prepare": "lefthook install || echo 'Can not set git hooks'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "firebase": "^12.6.0",
    "konva": "^10.0.12",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-konva": "^19.2.1",
    "react-router-dom": "^7.10.1",
    "use-image": "^1.1.4"
  },
  "devDependencies": {
    "@types/node": "^22.19.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "firebase-tools": "^14.27.0",
    "globals": "^16.5.0",
    "lefthook": "^2.0.8",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
